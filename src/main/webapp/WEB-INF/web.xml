<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
 <display-name>Contacts Manager Web Application</display-name>
   <servlet>
       <servlet-name>contactDaoImpl</servlet-name>
       <servlet-class>dao.ContactDaoImpl</servlet-class>
   </servlet>
   <servlet>
       <servlet-name>contactsDao</servlet-name>
       <servlet-class>dao.ContactsDao</servlet-class>
   </servlet>
   <servlet>
       <servlet-name>loginDao</servlet-name>
       <servlet-class>dao.LoginDao</servlet-class>
   </servlet>
   <servlet>
       <servlet-name>userDao</servlet-name>
       <servlet-class>dao.UserDao</servlet-class>
   </servlet>
   <servlet>
       <servlet-name>Contacts</servlet-name>
       <servlet-class>model.Contacts</servlet-class>
   </servlet>
   <servlet>
       <servlet-name>login</servlet-name>
       <servlet-class>model.Login</servlet-class>
   </servlet>
   <servlet>
       <servlet-name>user</servlet-name>
       <servlet-class>model.User</servlet-class>
   </servlet>
   <servlet>
       <servlet-name>JDBCutils</servlet-name>
       <servlet-class>utils.JDBCutils</servlet-class>
   </servlet>
   <servlet>
       <servlet-name>contactsController</servlet-name>
       <servlet-class>web.ContactsController</servlet-class>
   </servlet>
   <servlet>
       <servlet-name>loginControlle</servlet-name>
       <servlet-class>web.LoginController</servlet-class>
   </servlet>
   <servlet>
       <servlet-name>userController</servlet-name>
       <servlet-class>web.UserController</servlet-class>
   </servlet>

  <servlet-mapping>
       <servlet-name>contactsDaoImpl</servlet-name>
       <url-pattern>/contactsDaImpl</url-pattern>
   </servlet-mapping>
    <servlet-mapping>
       <servlet-name>contactsDao</servlet-name>
       <url-pattern>/contacsDao</url-pattern>
   </servlet-mapping>
    <servlet-mapping>
       <servlet-name>loginDao</servlet-name>
       <url-pattern>/loginDao</url-pattern>
   </servlet-mapping>
    <servlet-mapping>
       <servlet-name>userDao</servlet-name>
       <url-pattern>/userDao</url-pattern>
   </servlet-mapping>
    <servlet-mapping>
       <servlet-name>contacts</servlet-name>
       <url-pattern>/contacts</url-pattern>
   </servlet-mapping>
    <servlet-mapping>
       <servlet-name>login</servlet-name>
       <url-pattern>/login</url-pattern>
   </servlet-mapping>
    <servlet-mapping>
       <servlet-name>user</servlet-name>
       <url-pattern>/user</url-pattern>
   </servlet-mapping>
    <servlet-mapping>
       <servlet-name>JDBCutils</servlet-name>
       <url-pattern>/JDBCutils</url-pattern>
   </servlet-mapping>
    <servlet-mapping>
       <servlet-name>contactsController</servlet-name>
       <url-pattern>/hello</url-pattern>
   </servlet-mapping>
    <servlet-mapping>
       <servlet-name>loginController</servlet-name>
       <url-pattern>/loginController</url-pattern>
   </servlet-mapping>
    <servlet-mapping>
       <servlet-name>userController</servlet-name>
       <url-pattern>/userController</url-pattern>
   </servlet-mapping>
    <error-page>
  <exception-type>java.lang.Exception</exception-type>
  <location>/Error.jsp</location>
 </error-page>
 <welcome-file-list>
 <welcome-file>index.html</welcome-file>
        <welcome-file>index.jsp</welcome-file>
       <welcome-file>login.jsp</welcome-file>
       <welcome-file>register.jsp</welcome-file>
       <welcome-file>footer.jsp</welcome-file>
       <welcome-file>header.jsp</welcome-file>
       <welcome-file>todo-form.jsp</welcome-file>
       <welcome-file>todo-list.jsp</welcome-file>
       
 </welcome-file-list>
</web-app>